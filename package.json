{
    "name": "oro-theme",
    "version": "2.2.1",
    "displayName": "Oro Theme",
    "description": "A dark mode theme that aims to be both functional and aesthetically pleasing.",
    "publisher": "deimonn",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/deimonn/oro-theme"
    },
    "icon": "assets/logo.png",
    "keywords": [
        "dark",
        "theme",
        "oro",
        "white",
        "gold",
        "teal",
        "rust",
        "c",
        "cpp",
        "c++",
        "csharp",
        "c#"
    ],
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Oro Theme",
                "uiTheme": "vs-dark",
                "path": "./dist/mainTheme.json"
            },
            {
                "label": "Oro Theme (Italics)",
                "uiTheme": "vs-dark",
                "path": "./dist/mainThemeItalics.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "ts-node src/generate.ts",
        "lint": "eslint src --ext ts",
        "deploy": "vsce publish",
        "deployPreview": "vsce publish --pre-release"
    },
    "devDependencies": {
        "@types/color": "^3.0.6",
        "@types/node": "20.x",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "@vscode/vsce": "^2.22.0",
        "color": "^4.2.3",
        "eslint": "^8.55.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.2"
    }
}
